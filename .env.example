# Cloudflare 配置
CLOUDFLARE_ACCOUNT_ID=your-account-id
CLOUDFLARE_API_TOKEN=your-api-token

# 数据库配置
WAHA_AGENT_DATABASE_ID=your-d1-database-id
WAHA_AGENT_KV_NAMESPACE_ID=your-kv-namespace-id
WAHA_AGENT_R2_BUCKET_NAME=your-r2-bucket-name
WAHA_AGENT_VECTORIZE_INDEX_NAME=your-vectorize-index-name

# 认证配置（使用 wrangler secret put 设置）
BETTER_AUTH_SECRET=your-super-secret-key-32-chars
GOOGLE_CLIENT_ID=your-google-oauth-client-id
GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret

# 加密密钥（使用 wrangler secret put 设置）
ENCRYPTION_KEY=your-32-byte-encryption-key-here

# 管理员邮箱（使用 wrangler secret put 设置）
ADMIN_EMAILS=admin@example.com,admin2@example.com

# 开发环境配置
ENVIRONMENT=development